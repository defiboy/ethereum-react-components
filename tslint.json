{
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "rules": {
        "interface-name": false,
        "object-literal-sort-keys": false,
        "no-console": false
    },
    "linterOptions": {
        "exclude": [
            "node_modules/**/*.ts",
            "coverage/**/*.js",
            "src/**/*.json"
        ]
    }
}